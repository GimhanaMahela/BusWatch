# .env in buswatch-frontend

# Backend API URL (ensure this matches your backend's port)
REACT_APP_BACKEND_URL=http://localhost:5000/api

# Google Maps API Key (for MapPicker component)
# Get one from Google Cloud Console: https://console.cloud.google.com/
# Make sure to enable "Maps JavaScript API" and restrict the key to your domain (e.g., http://localhost:3000/*)
REACT_APP_Maps_API_KEY=YOUR_Maps_API_KEY
